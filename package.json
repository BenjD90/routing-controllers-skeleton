{
  "name": "n9-node-routing",
  "version": "1.25.0",
  "description": "Library to easily start app with routing-controllers",
  "keywords": [
    "node",
    "express",
    "decorators",
    "routing controllers",
    "typescript",
    "anotation"
  ],
  "license": "GNU",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "build": "rimraf dist/ && tsc",
    "format": "prettier --write '{,@(src|test)/**/}*.*'",
    "lint": "tslint --project tsconfig.json --format verbose '{src,test}/**/*.ts'",
    "lint:apply": "npm run format && tslint --fix --project tsconfig.json '{src,test}/**/*.ts'",
    "lint:verbose": "tslint --fix --project tsconfig.json --format verbose '{src,test}/**/*.ts'",
    "test:dev": "export NODE_ENV=test && TS_NODE_FILES=true ava --verbose --color --serial --watch",
    "test": "npm run lint && npm run build && export NODE_ENV=test && nyc ava --verbose --color --serial && nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov -f coverage.lcov",
    "preversion": "npm test",
    "prepublishOnly": "npm run build",
    "release": "export BABEL_DISABLE_CACHE=1 && release-it --ci",
    "release:dry": "export BABEL_DISABLE_CACHE=1 && release-it --ci --dry-run=true",
    "post:release": "echo Successfully released $npm_package_version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "author": {
    "name": "Benjamin DANIEL",
    "email": "benjamin.daniel@neo9.fr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neo9/n9-node-routing.git"
  },
  "homepage": "https://github.com/neo9/n9-node-routing#n9-node-routing",
  "tags": [
    "express",
    "typescript-express",
    "routing-controllers"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@benjd90/routing-controllers-openapi": "^1.7.0",
    "@flyacts/routing-controllers": "^0.9.2",
    "@neo9/n9-node-log": "^3.0.0",
    "@neo9/n9-node-utils": "^1.9.0",
    "@promster/express": "^4.1.12",
    "@promster/server": "^4.2.13",
    "app-root-dir": "^1.0.2",
    "async": "^3.2.0",
    "body-parser": "^1.17.2",
    "class-validator-jsonschema": "^1.3.0",
    "continuation-local-storage": "^3.2.1",
    "express": "^4.15.3",
    "fast-safe-stringify": "^2.0.6",
    "form-data": "^3.0.0",
    "glob": "^7.1.2",
    "glob-promise": "^3.4.0",
    "got": "^11.7.0",
    "helmet": "^4.1.1",
    "moment": "^2.29.0",
    "morgan": "1.10.0",
    "multer": "^1.4.2",
    "prom-client": "^12.0.0",
    "query-string": "^6.13.2",
    "reflect-metadata": "^0.1.10",
    "shortid": "^2.2.8",
    "stream-array": "^1.1.2",
    "swagger-ui-express": "^4.1.4",
    "typedi": "^0.8.0",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@commitlint/cli": "^11.0.0",
    "@neo9/n9-coding-style": "^1.1.1",
    "@neo9/n9-mongo-client": "^0.15.1",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/app-root-dir": "^0.1.0",
    "@types/body-parser": "^1.16.3",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/express": "^4.17.8",
    "@types/helmet": "^0.0.48",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.11.2",
    "@types/shortid": "^0.0.29",
    "@types/std-mocks": "^1.0.0",
    "@types/url-join": "^4.0.0",
    "ava": "^3.12.1",
    "codecov": "^3.7.2",
    "conventional-changelog-cli": "^2.1.0",
    "husky": "^4.3.0",
    "mongodb-memory-server": "^6.8.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "release-it": "^14.0.3",
    "std-mocks": "^1.0.1",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.0.3"
  },
  "ava": {
    "files": [
      "test/*"
    ],
    "timeout": "1m",
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "ignoredByWatcher": [
      "**/openapi-documentation.json"
    ]
  }
}
