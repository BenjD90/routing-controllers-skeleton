{
  "name": "routing-controllers-wrapper",
  "version": "0.0.1",
  "description": "Express app skeleton based on routing-controllers",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist/ && tsc",
    "lint": "tslint --project tsconfig.json",
    "test": "npm run lint  && npm run build && nyc ava --verbose --serial dist/test/ && nyc report --reporter=html",
    "test:dev": "tslint --project tsconfig.json --force && npm run build && nyc ava --verbose --serial dist/test/ && nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov -t b2eaf76a-9a9e-4190-aa8f-398a8ff33939",
    "preversion": "npm test",
    "prepublishOnly": "npm run build"
  },
  "author": {
    "name": "Benjamin DANIEL",
    "email": "benjamin.b.daniel@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BenjD90/routing-controllers-skeleton.git"
  },
  "tags": [
    "express",
    "typescript-express",
    "routing-controllers"
  ],
  "dependencies": {
    "@neo9/n9-node-log": "^2.4.0",
    "@neo9/n9-node-utils": "^1.5.0",
    "@types/app-root-dir": "^0.1.0",
    "@types/helmet": "^0.0.37",
    "@types/morgan": "^1.7.35",
    "app-root-dir": "^1.0.2",
    "body-parser": "^1.17.2",
    "class-validator": "^0.8.5",
    "codecov": "^3.0.0",
    "express": "^4.15.3",
    "glob-promise": "^3.4.0",
    "helmet": "^3.12.0",
    "moment": "^2.22.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "reflect-metadata": "^0.1.10",
    "routing-controllers": "^0.7.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typedi": "^0.5.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/express": "^4.0.35",
    "@types/multer": "^0.0.33",
    "@types/node": "^7.0.22",
    "@types/request-promise-native": "^1.0.14",
    "@types/std-mocks": "^1.0.0",
    "ava": "^1.0.0-beta.3",
    "nodemon": "^1.17.3",
    "nyc": "^11.6.0",
    "request-promise-native": "^1.0.5",
    "std-mocks": "^1.0.1",
    "typescript": "^2.3.3"
